Options -Indexes
Options +FollowSymLinks
RewriteEngine On
RewriteRule ^PlanetaMercurio.jsp$ mercurio.php

# Controller de eliminar Usuario
RewriteRule ^DelProveedor.jsp$ Controller/Proveedor/delProveedor.php
RewriteRule ^BuscarProXId.jsp$ Controller/Proveedor/BuscarXiDPro.php
RewriteRule ^ForModiProXId.jsp$ View/Proveedor/ForModificar.php

RewriteRule ^generarTabla$ Controller/Proveedor/generarTabla.php
RewriteRule ^SubirFoto$ Controller/cargarFoto/UnLoadFoto.php


RewriteRule ^ADDProducto$ Controller/producto/add.php
RewriteRule ^LisProXCat$ Controller/producto/BuscarXCat.php


<files .htaccess>
order allow,deny
deny from all
</files>